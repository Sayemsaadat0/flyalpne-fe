{
  "compilerOptions": {
    "allowJs": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "baseUrl": ".",
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./out",
    "preserveSymlinks": true,
    "pretty": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "rootDirs": ["public", "src"],
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES2017",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@app/*": ["./src/app/*"],
      "@components/*": ["./src/components/*"],
      "@data/*": ["./src/data/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@images/*": ["./public/images/*"],
      "@libs/*": ["./src/libs/*"],
      "@models/*": ["./src/models/*"],
      "@styles/*": ["./src/styles/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "out/types/**/*.ts",
    "src/app/(root)/product/[slug]",
    "src/components/ui/accordion.jsx",
    "src/app/(root)/products/_components/Filter Products/ProductFilters.jsx",
    "src/app/(root)/products/_components/AllProducts.jsx",
    "src/app/(root)/products/_components/Filter Products/PriceRangeSlider.jsx",
    "src/app/(root)/_components/HomeCategorySection.tsx",
    "src/app/(root)/singleSeller/_components/filter-sort.tsx",
    "src/app/(seller)/seller/forget-password/_component/ForgetPasswordContainer.tsx"
  ],
  "exclude": ["node_modules", ".next", "out", "dist", "*.d.ts"],
  "types": [
    "types",
    "node",
    "react",
    "react-dom",
    "tailwindcss",
    "commitlint/types",
    "react-datepicker",
    "google.maps"
  ]
}
